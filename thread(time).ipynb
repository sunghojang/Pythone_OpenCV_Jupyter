   "source": [
    "import threading\n",
    "\n",
    "def sum(low, high):\n",
    "    total = 0\n",
    "    for i in range(low, high):\n",
    "        total += i\n",
    "    print(\"Subthread\", total) \n",
    "t = threading.Thread(target=sum, args=(1, 100000))\n",
    "t.start()\n",
    " \n",
    "print(\"Main Thread\")"
   ]
